<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.ssh.lose.po.Letter" table="Letter">
		<id name="id" type="java.lang.Integer">
			<generator class="increment" />
		</id>
		<property name="user_id" type="java.lang.Integer"></property>
		<property name="title" type="java.lang.String" />
		<property name="toname" type="java.lang.String" />
		<property name="thankinfo" type="java.lang.String"/>
		<property name="create_time" type="java.lang.String"/>
	</class>
</hibernate-mapping>