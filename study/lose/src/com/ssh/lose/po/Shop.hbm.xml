<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.ssh.lose.po.Shop" table="Shop">
		<id name="id" type="java.lang.Integer">
			<generator class="increment" />
		</id>
		<property name="user_id" type="java.lang.Integer" />
		<property name="title" type="java.lang.String" />
		<property name="shop_type" type="java.lang.String" />
		<property name="shop_name" type="java.lang.String" />
		<property name="shop_time" type="java.lang.String" />
		<property name="address" type="java.lang.String" />
		<property name="telphone" type="java.lang.String" />
		<property name="qq" type="java.lang.String" />
		<property name="detail" type="java.lang.String" />
		<property name="pic" type="java.lang.String" />
		<property name="state" type="java.lang.String" />
		<property name="comment" type="java.lang.String" />
		<property name="type" type="java.lang.String" />
		<property name="create_time" type="java.lang.String" />
		<property name="last_update_time" type="java.lang.String" />
	</class>
</hibernate-mapping>