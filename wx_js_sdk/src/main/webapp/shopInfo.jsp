</script>

<script>
	//通过config接口注入权限验证配置
	wx.config({
		debug : true, // 开启调试模式,调用的所有api的返回值会在客户端alert出来，若要查看传入的参数，可以在pc端打开，参数信息会通过log打出，仅在pc端时才会打印。
		appId : '${appid}', // 必填，公众号的唯一标识
		timestamp : '${timestamp}', // 必填，生成签名的时间戳
		nonceStr : '${nonceStr}', // 必填，生成签名的随机串
		signature : '${signature}', // 必填，签名，见附录1
		jsApiList : [ 'onMenuShareTimeline', 'onMenuShareAppMessage',
				'onMenuShareQQ', 'onMenuShareWeibo', 'onMenuShareWeibo',
				'onMenuShareQZone' ]
	// 必填，需要使用的JS接口列表，所有JS接口列表见附录2
	});

	wx.ready(function() {
		shareTo();
		shareToFriend();
		shareToQQ();
		shareToWB();
		shareToQQKj();
		// config信息验证后会执行ready方法，所有接口调用都必须在config接口获得结果之后
	});
	wx.error(function(res) {
		// config信息验证失败会执行error函数，如签名过期导致验证失败，具体错误信息可以打开config的debug模式查看，也可以在返回的res参数中查看，对于SPA可以在这里更新签名。
	});

	var title = '♥感恩回馈♥好礼不停♥'; // 分享标题
	var link = window.location.href; // 分享链接，该链接域名或路径必须与当前页面对应的公众号JS安全域名一致
	var imgUrl = 'http://ip.freep.cn/590589/%E6%AC%BE%E5%BC%8Fb.jpg';
	var desc = '真实有效'; // 分享描述
	var qtitle = '♥感恩回馈♥好礼不停♥';
	var qlink = window.location.href;
	var qimgUrl = 'http://ip.freep.cn/590589/%E6%AC%BE%E5%BC%8Fb.jpg';

	function shareTo() {
		//获取“分享到朋友圈”按钮点击状态及自定义分享内容接口
		wx.onMenuShareTimeline({
			'title' : title,
			'link' : link,
			'imgUrl' : imgUrl,
			'desc' : desc,
			'qtitle' : qtitle,
			'qlink' : qlink,
			'qimgUrl' : qimgUrl,
			success : function(res) {
				toUrl(res);
			}
		});
	}

	function shareToFriend() {
		//获取“分享给朋友”按钮点击状态及自定义分享内容接口
		wx.onMenuShareAppMessage({
			'title' : title,
			'link' : link,
			'imgUrl' : imgUrl,
			'desc' : desc,
			'qtitle' : qtitle,
			'qlink' : qlink,
			'qimgUrl' : qimgUrl,
			success : function(res) {
				toUrl(res);
			},
			fail : function(res) {
			}
		});

	}

	//获取“分享到QQ”按钮点击状态及自定义分享内容接口
	function shareToQQ() {
		wx.onMenuShareQQ({
			'title' : title,
			'link' : link,
			'imgUrl' : imgUrl,
			'desc' : desc,
			'qtitle' : qtitle,
			'qlink' : qlink,
			'qimgUrl' : qimgUrl,
			success : function(res) {
				toUrl(res);
			}
		});
	}

	//获取“分享到腾讯微博”按钮点击状态及自定义分享内容接口
	function shareToWB() {
		wx.onMenuShareWeibo({
			'title' : title,
			'link' : link,
			'imgUrl' : imgUrl,
			'desc' : desc,
			'qtitle' : qtitle,
			'qlink' : qlink,
			'qimgUrl' : qimgUrl,
			success : function(res) {
				toUrl(res);
			}
		});
	}

	//获取“分享到QQ空间”按钮点击状态及自定义分享内容接口
	function shareToQQKj() {
		wx.onMenuShareQZone({
			'title' : title,
			'link' : link,
			'imgUrl' : imgUrl,
			'desc' : desc,
			'qtitle' : qtitle,
			'qlink' : qlink,
			'qimgUrl' : qimgUrl,
			success : function(res) {
				toUrl(res);
			}
		});
	}
	function toUrl(res) {
		window.location.href = "${jurl}";
	}
</script>
